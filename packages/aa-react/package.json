{
  "name": "@bitlayer/aa-react",
  "version": "0.1.0",
  "description": "React components and hooks for interacting with ERC-4337 Smart Accounts on Bitlayer.",
  "author": "Bitlayer",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bitlayer-org/bitlayer-aa-sdk.git",
    "directory": "packages/aa-react"
  },
  "type": "module",
  "scripts": {
    "build": "pnpm run clean && pnpm run build:esm+types",
    "build:esm+types": "tsc --project tsconfig.build.json --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "/hooks",
    "/providers"
  ],
  "main": "./dist/esm/exports/index.js",
  "types": "./dist/types/exports/index.d.ts",
  "typings": "./dist/types/exports/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/exports/index.d.ts",
      "default": "./dist/esm/exports/index.js"
    },
    "./hooks": {
      "types": "./dist/types/exports/hooks.d.ts",
      "default": "./dist/esm/exports/hooks.js"
    },
    "./providers": {
      "types": "./dist/types/exports/providers.d.ts",
      "default": "./dist/esm/exports/providers.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@aa-sdk/core": "^4.1.1",
    "@bitlayer/aa-sdk": "workspace:*"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@tanstack/react-query": "^5.59.15",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "react": ">=18",
    "typescript": ">=5.0.4",
    "viem": "2.x"
  }
}
